{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|json|txt|pdf|woff2?)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
